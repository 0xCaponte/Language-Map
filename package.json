{
	"name": "language-map",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"preprocess": "vite-node ./scripts/PreprocessCountryData.ts",
		"dev": "vite dev",
		"full-dev": "npm run preprocess && vite dev",
		"build": "npm run preprocess && vite build",
		"build:cloudflare": "npm run preprocess  && echo 'Preprocessing complete, starting build...' &&  cross-env ADAPTER=cloudflare vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@playwright/test": "^1.41.2",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/adapter-cloudflare": "^4.1.0",
		"@sveltejs/kit": "^2.17.3",
		"autoprefixer": "^10.4.17",
		"cross-env": "^7.0.3",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite": "^2.2.1",
		"flowbite-svelte": "^0.48.4",
		"flowbite-svelte-icons": "2.0.2",
		"postcss": "^8.4.33",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.2.4",
		"prettier-plugin-svelte": "^3.1.2",
		"super-sitemap": "^0.14.14",
		"svelte": "^5.21.0",
		"svelte-check": "^3.6.3",
		"svelte-meta-tags": "^4.2.0",
		"tailwind-scrollbar": "^3.0.5",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"vite": "^6.2.0",
		"vitest": "^1.2.2",
		"wrangler": "^3.26.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"d3": "^7.8.5",
		"d3-geo": "^3.1.0",
		"geojson": "^0.5.0",
		"node-fetch": "^3.3.2",
		"topojson-client": "^3.1.0"
	}
}
